	activeData : REFERENCE TO ULINT REF= localData;
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[]]></ST>
    </Implementation>
    <Folder Name="Constructor" Id="{c056bdcf-aa40-02f2-177e-270978828ede}" />
    <Property Name="Address" Id="{e633fbea-6deb-07a1-0f6b-d3d8dd5ece97}">
